

VGBIOMETH_T.UP(IY411,IA,IGDISPATCH(IHYDROGEN_GH2TOBIOMETH),S,T)$(GKFX(IY411,IA,IHYDROGEN_GH2TOBIOMETH) AND (NOT SUM(Y$(YVALUE(Y) LE YVALUE(IY411)),IAGKNY(Y,IA,IHYDROGEN_GH2TOBIOMETH))) AND (NOT (IGDECOMEXOPOT(IY411,IA,IHYDROGEN_GH2TOBIOMETH)))) = GKFX(IY411,IA,IHYDROGEN_GH2TOBIOMETH)*(1$(NOT IGKRATE(IA,IHYDROGEN_GH2TOBIOMETH,S,T)) + IGKRATE(IA,IHYDROGEN_GH2TOBIOMETH,S,T));

