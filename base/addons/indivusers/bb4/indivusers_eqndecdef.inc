*Individual users addon


$ifi not %GROUP_DEMAND_SHIFT%==yes $goto NO_GROUP_DEMAND_SHIFT
EQUATIONS
QINDIVUSERS_DH(Y,RRR,DHUSER)                          "Individual users: all annual heat per heat user demand must be covered (MWh)"
QINDIVUSERS_GROUP_MIN(Y,RRR,DHUSER,INDIVUSERS_GROUP)  "Individual users: minimum demand per heat user that needs to be satisfied by a group (MWh)"
QINDIVUSERS_GROUP_MAX(Y,RRR,DHUSER,INDIVUSERS_GROUP)  "Individual users: maximum demand per heat user that needs to be satisfied by a group (MWh)"
;

QINDIVUSERS_DH(IY411,IR,DHUSER)$SUM(IA$(RRRAAA(IR,IA) AND INDIVUSERS_AAA(IA)),DH(IY411,IA,DHUSER))..
SUM(INDIVUSERS_GROUP$SUM(IA$(RRRAAA(IR,IA) AND INDIVUSERS_AAA(IA)),INDIVUSERS_GROUP_A(IA,DHUSER,INDIVUSERS_GROUP)),VINDIVUSERS_GROUPSHARE(IY411,IR,DHUSER,INDIVUSERS_GROUP))
     =E=
1
;

QINDIVUSERS_GROUP_MIN(IY411,IR,DHUSER,INDIVUSERS_GROUP)$(INDIVUSERS_MINSHARE_DH(IY411,IR,DHUSER,INDIVUSERS_GROUP))..
VINDIVUSERS_GROUPSHARE(IY411,IR,DHUSER,INDIVUSERS_GROUP)
     =G=
INDIVUSERS_MINSHARE_DH(IY411,IR,DHUSER,INDIVUSERS_GROUP)
;

QINDIVUSERS_GROUP_MAX(IY411,IR,DHUSER,INDIVUSERS_GROUP)$(INDIVUSERS_MAXSHARE_DH(IY411,IR,DHUSER,INDIVUSERS_GROUP))..
VINDIVUSERS_GROUPSHARE(IY411,IR,DHUSER,INDIVUSERS_GROUP)
     =L=
INDIVUSERS_MAXSHARE_DH(IY411,IR,DHUSER,INDIVUSERS_GROUP)
;
$label NO_GROUP_DEMAND_SHIFT 





















