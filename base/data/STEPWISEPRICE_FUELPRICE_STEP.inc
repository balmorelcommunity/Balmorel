/
2016 .DENMARK .L1 .WOODCHIPS  0.670
2016 .DENMARK .L2 .WOODCHIPS  0.766
2016 .DENMARK .L3 .WOODCHIPS  0.875
2016 .DENMARK .L4 .WOODCHIPS  1.000
2016 .DENMARK .L5 .WOODCHIPS  1.125
2016 .DENMARK .L6 .WOODCHIPS  1.266
2016 .DENMARK .L7 .WOODCHIPS  1.424
2016 .DENMARK .L8 .WOODCHIPS  14.238
/;
STEPWISEPRICE_FUELPRICE_STEP(Y,C,STEPWISEPRICE_L,STEPWISEPRICE_FFF)=STEPWISEPRICE_FUELPRICE_STEP('2016','DENMARK',STEPWISEPRICE_L,'WOODCHIPS')*SUM(IA$ICA(C,IA),FUELPRICE(Y,IA,STEPWISEPRICE_FFF))*1/SUM(IA$ICA(C,IA),1);